{
  "name": "expense-manager-fsd",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Expense Manager PWA built with Feature-Sliced Design architecture",
  "keywords": ["pwa", "expense-manager", "fsd", "feature-sliced-design"],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check . --apply",
    "lint:fsd": "steiger src",
    "format": "biome format . --write",
    "type-check": "tsc --noEmit",
    "verify": "pnpm format && pnpm lint && pnpm lint:fsd && pnpm type-check && pnpm build && pnpm cap:sync",
    "prepare": "husky",
    "cap:sync": "pnpm build && npx cap sync",
    "cap:sync:ios": "pnpm build && npx cap sync ios",
    "cap:sync:android": "pnpm build && npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "ios": "pnpm cap:sync:ios && pnpm cap:open:ios",
    "android": "pnpm cap:sync:android && pnpm cap:open:android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
